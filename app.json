{
  "expo": {
    "name": "Parking Finder",
    "slug": "parking-finder",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Parking Finder to access your location."
        }
      ],
      "expo-router",
      "expo-web-browser"
    ],
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "android.permission.INTERNET"
      ]
    }
  }
}
